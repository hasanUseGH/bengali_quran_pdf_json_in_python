[
    {
        "id": "5759",
        "sura": "80",
        "aya": "1",
        "text": "তিনি ভ্রূকুঞ্চিত করলেন এবং মুখ ফিরিয়ে নিলেন।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5760",
        "sura": "80",
        "aya": "2",
        "text": "কারণ, তাঁর কাছে এক অন্ধ আগমন করল।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5761",
        "sura": "80",
        "aya": "3",
        "text": "আপনি কি জানেন, সে হয়তো পরিশুদ্ধ হত,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5762",
        "sura": "80",
        "aya": "4",
        "text": "অথবা উপদেশ গ্রহণ করতো এবং উপদেশ তার উপকার হত।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5763",
        "sura": "80",
        "aya": "5",
        "text": "পরন্তু যে বেপরোয়া,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5764",
        "sura": "80",
        "aya": "6",
        "text": "আপনি তার চিন্তায় মশগুল।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5765",
        "sura": "80",
        "aya": "7",
        "text": "সে শুদ্ধ না হলে আপনার কোন দোষ নেই।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5766",
        "sura": "80",
        "aya": "8",
        "text": "যে আপনার কাছে দৌড়ে আসলো",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5767",
        "sura": "80",
        "aya": "9",
        "text": "এমতাবস্থায় যে, সে ভয় করে,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5768",
        "sura": "80",
        "aya": "10",
        "text": "আপনি তাকে অবজ্ঞা করলেন।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5769",
        "sura": "80",
        "aya": "11",
        "text": "কখনও এরূপ করবেন না, এটা উপদেশবানী।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5770",
        "sura": "80",
        "aya": "12",
        "text": "অতএব, যে ইচ্ছা করবে, সে একে গ্রহণ করবে।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5771",
        "sura": "80",
        "aya": "13",
        "text": "এটা লিখিত আছে সম্মানিত,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5772",
        "sura": "80",
        "aya": "14",
        "text": "উচ্চ পবিত্র পত্রসমূহে,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5773",
        "sura": "80",
        "aya": "15",
        "text": "লিপিকারের হস্তে,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5774",
        "sura": "80",
        "aya": "16",
        "text": "যারা মহৎ, পূত চরিত্র।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5775",
        "sura": "80",
        "aya": "17",
        "text": "মানুষ ধ্বংস হোক, সে কত অকৃতজ্ঞ!",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5776",
        "sura": "80",
        "aya": "18",
        "text": "তিনি তাকে কি বস্তু থেকে সৃষ্টি করেছেন?",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5777",
        "sura": "80",
        "aya": "19",
        "text": "শুক্র থেকে তাকে সৃষ্টি করেছেন, অতঃপর তাকে সুপরিমিত করেছেন।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5778",
        "sura": "80",
        "aya": "20",
        "text": "অতঃপর তার পথ সহজ করেছেন,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5779",
        "sura": "80",
        "aya": "21",
        "text": "অতঃপর তার মৃত্যু ঘটান ও কবরস্থ করেন তাকে।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5780",
        "sura": "80",
        "aya": "22",
        "text": "এরপর যখন ইচ্ছা করবেন তখন তাকে পুনরুজ্জীবিত করবেন।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5781",
        "sura": "80",
        "aya": "23",
        "text": "সে কখনও কৃতজ্ঞ হয়নি, তিনি তাকে যা আদেশ করেছেন, সে তা পূর্ণ করেনি।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5782",
        "sura": "80",
        "aya": "24",
        "text": "মানুষ তার খাদ্যের প্রতি লক্ষ্য করুক,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5783",
        "sura": "80",
        "aya": "25",
        "text": "আমি আশ্চর্য উপায়ে পানি বর্ষণ করেছি,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5784",
        "sura": "80",
        "aya": "26",
        "text": "এরপর আমি ভূমিকে বিদীর্ণ করেছি,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5785",
        "sura": "80",
        "aya": "27",
        "text": "অতঃপর তাতে উৎপন্ন করেছি শস্য,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5786",
        "sura": "80",
        "aya": "28",
        "text": "আঙ্গুর, শাক-সব্জি,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5787",
        "sura": "80",
        "aya": "29",
        "text": "যয়তুন, খর্জূর,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5788",
        "sura": "80",
        "aya": "30",
        "text": "ঘন উদ্যান,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5789",
        "sura": "80",
        "aya": "31",
        "text": "ফল এবং ঘাস",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5790",
        "sura": "80",
        "aya": "32",
        "text": "তোমাদেরও তোমাদের চতুস্পদ জন্তুদের উপাকারার্থে।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5791",
        "sura": "80",
        "aya": "33",
        "text": "অতঃপর যেদিন কর্ণবিদারক নাদ আসবে,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5792",
        "sura": "80",
        "aya": "34",
        "text": "সেদিন পলায়ন করবে মানুষ তার ভ্রাতার কাছ থেকে,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5793",
        "sura": "80",
        "aya": "35",
        "text": "তার মাতা, তার পিতা,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5794",
        "sura": "80",
        "aya": "36",
        "text": "তার পত্নী ও তার সন্তানদের কাছ থেকে।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5795",
        "sura": "80",
        "aya": "37",
        "text": "সেদিন প্রত্যেকেরই নিজের এক চিন্তা থাকবে, যা তাকে ব্যতিব্যস্ত করে রাখবে।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5796",
        "sura": "80",
        "aya": "38",
        "text": "অনেক মুখমন্ডল সেদিন হবে উজ্জ্বল,",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5797",
        "sura": "80",
        "aya": "39",
        "text": "সহাস্য ও প্রফুল্ল।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5798",
        "sura": "80",
        "aya": "40",
        "text": "এবং অনেক মুখমন্ডল সেদিন হবে ধুলি ধূসরিত।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5799",
        "sura": "80",
        "aya": "41",
        "text": "তাদেরকে কালিমা আচ্ছন্ন করে রাখবে।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    },
    {
        "id": "5800",
        "sura": "80",
        "aya": "42",
        "text": "তারাই কাফের পাপিষ্ঠের দল।",
        "englishName": "Abasa",
        "englishNameTranslation": "He frowned"
    }
]