[
    {
        "id": "6158",
        "sura": "101",
        "aya": "1",
        "text": "করাঘাতকারী,",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6159",
        "sura": "101",
        "aya": "2",
        "text": "করাঘাতকারী কি?",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6160",
        "sura": "101",
        "aya": "3",
        "text": "করাঘাতকারী সম্পর্কে আপনি কি জানেন ?",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6161",
        "sura": "101",
        "aya": "4",
        "text": "যেদিন মানুষ হবে বিক্ষিপ্ত পতংগের মত",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6162",
        "sura": "101",
        "aya": "5",
        "text": "এবং পর্বতমালা হবে ধুনিত রঙ্গীন পশমের মত।",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6163",
        "sura": "101",
        "aya": "6",
        "text": "অতএব যার পাল্লা ভারী হবে,",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6164",
        "sura": "101",
        "aya": "7",
        "text": "সে সুখীজীবন যাপন করবে।",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6165",
        "sura": "101",
        "aya": "8",
        "text": "আর যার পাল্লা হালকা হবে,",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6166",
        "sura": "101",
        "aya": "9",
        "text": "তার ঠিকানা হবে হাবিয়া।",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6167",
        "sura": "101",
        "aya": "10",
        "text": "আপনি জানেন তা কি?",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    },
    {
        "id": "6168",
        "sura": "101",
        "aya": "11",
        "text": "প্রজ্জ্বলিত অগ্নি!",
        "englishName": "Al-Qaari'a",
        "englishNameTranslation": "The Calamity"
    }
]